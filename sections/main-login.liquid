{{ 'customer.css' | asset_url | stylesheet_tag }}

{%- style -%}
  /* Login section background - scoped to this section only */
  #shopify-section-{{ section.id }} {
    background-color: {{ section.settings.section_bg_color }} !important;
    width: 100% !important;
    max-width: 100% !important;
    margin: 0 !important;
    padding: {{ section.settings.section_padding_top }}px 0 {{ section.settings.section_padding_bottom }}px 0 !important;
    min-height: auto !important;
  }

  /* Login section styling - customizable container */
  .customer.login.section-{{ section.id }}-padding {
    max-width: {{ section.settings.container_max_width }}px !important;
    margin: {{ section.settings.container_margin_top }}px auto {{ section.settings.container_margin_bottom }}px auto !important;
    background: {{ section.settings.container_bg_color }} !important;
    padding: {{ section.settings.container_padding }}px !important;
    border-radius: {{ section.settings.container_border_radius }}px !important;
    box-shadow: {% if section.settings.show_shadow %}0 4px 20px rgba(0, 0, 0, 0.08){% else %}none{% endif %} !important;
    text-align: {{ section.settings.text_alignment }} !important;
  }

  /* Hide logo as requested */
  .customer.login .login-logo {
    display: none !important;
  }

  /* Heading styling */
  .customer.login h1 {
    text-align: {{ section.settings.text_alignment }};
    margin-bottom: {{ section.settings.heading_margin_bottom }}px;
    color: {{ section.settings.heading_color }};
    font-size: {{ section.settings.heading_font_size }}px;
    font-weight: {{ section.settings.heading_font_weight }};
  }

  /* Form fields */
  .customer.login .field {
    margin-bottom: {{ section.settings.field_margin_bottom }}px;
  }

  .customer.login .field input {
    border-radius: {{ section.settings.input_border_radius }}px !important;
    border: {{ section.settings.input_border_width }}px solid {{ section.settings.input_border_color }} !important;
    padding: {{ section.settings.input_padding }}px !important;
    font-size: {{ section.settings.input_font_size }}px !important;
    background-color: {{ section.settings.input_bg_color }} !important;
    color: {{ section.settings.input_text_color }} !important;
    transition: border-color 0.3s ease, box-shadow 0.3s ease !important;
  }

  .customer.login .field input:focus {
    border-color: {{ section.settings.input_focus_border_color }} !important;
    box-shadow: 0 0 0 3px {{ section.settings.input_focus_border_color | color_modify: 'alpha', 0.15 }} !important;
    outline: none !important;
  }

  .customer.login .field label {
    color: {{ section.settings.label_color }} !important;
    font-size: {{ section.settings.label_font_size }}px !important;
  }

  /* Links styling */
  .customer.login a {
    color: {{ section.settings.link_color }} !important;
    text-decoration: {{ section.settings.link_text_decoration }} !important;
    font-size: {{ section.settings.link_font_size }}px !important;
  }

  .customer.login a:hover {
    color: {{ section.settings.link_hover_color }} !important;
  }

  /* Button Styles - customizable */
  .customer.login button,
  .customer.login form button[type="submit"],
  .customer.login button[type="submit"],
  #login button,
  #recover button,
  #customer_login button,
  form[action*="login"] button {
    position: relative !important;
    display: inline-flex !important;
    align-items: center !important;
    justify-content: center !important;
    padding: {{ section.settings.button_padding_y }}px {{ section.settings.button_padding_x }}px !important;
    font-size: {{ section.settings.button_font_size }}px !important;
    font-weight: {{ section.settings.button_font_weight }} !important;
    letter-spacing: {{ section.settings.button_letter_spacing | divided_by: 100.0 }}em !important;
    text-decoration: none !important;
    text-transform: {{ section.settings.button_text_transform }} !important;
    border: {{ section.settings.button_border_width }}px solid {{ section.settings.button_border_color }} !important;
    border-radius: {{ section.settings.button_border_radius }}px !important;
    color: {{ section.settings.button_text_color }} !important;
    background: {{ section.settings.button_bg_color }} !important;
    cursor: pointer !important;
    transition: all {{ section.settings.button_transition_duration | divided_by: 10.0 }}s cubic-bezier(0.4, 0, 0.2, 1) !important;
    overflow: hidden !important;
    z-index: 1 !important;
    width: {% if section.settings.button_full_width %}100%{% else %}auto{% endif %} !important;
    margin: {{ section.settings.button_margin_top }}px 0 0 0 !important;
    min-height: auto !important;
    font-family: inherit !important;
    line-height: 1.5 !important;
  }

  {% if section.settings.button_gradient_hover %}
  .customer.login button::before,
  .customer.login form button[type="submit"]::before,
  #login button::before,
  #recover button::before {
    content: '' !important;
    position: absolute !important;
    top: 0 !important;
    left: 0 !important;
    width: 100% !important;
    height: 100% !important;
    background: linear-gradient(135deg, {{ section.settings.button_gradient_start }} 0%, {{ section.settings.button_gradient_middle }} 50%, {{ section.settings.button_gradient_end }} 100%) !important;
    background-size: 200% 200% !important;
    opacity: 0 !important;
    transition: opacity {{ section.settings.button_transition_duration | divided_by: 10.0 }}s cubic-bezier(0.4, 0, 0.2, 1) !important;
    z-index: -1 !important;
    border-radius: {{ section.settings.button_border_radius | minus: 2 }}px !important;
  }

  .customer.login button:hover::before,
  .customer.login form button[type="submit"]:hover::before,
  #login button:hover::before,
  #recover button:hover::before {
    opacity: 1 !important;
    background-position: 100% 0 !important;
    animation: mm-gradient-shift {{ section.settings.button_transition_duration | divided_by: 10.0 | times: 2 }}s ease forwards !important;
  }
  {% endif %}

  .customer.login button:hover,
  .customer.login form button[type="submit"]:hover,
  #login button:hover,
  #recover button:hover {
    color: {{ section.settings.button_hover_text_color }} !important;
    border-color: {{ section.settings.button_hover_border_color }} !important;
    transform: translateY({{ section.settings.button_hover_translate_y }}px) !important;
    box-shadow: {% if section.settings.button_hover_shadow %}0 8px 24px {{ section.settings.button_hover_shadow_color | color_modify: 'alpha', 0.3 }}{% else %}none{% endif %} !important;
    background: {% if section.settings.button_gradient_hover %}transparent{% else %}{{ section.settings.button_hover_bg_color }}{% endif %} !important;
  }

  @keyframes mm-gradient-shift {
    0% { background-position: 0% 50%; }
    100% { background-position: 100% 50%; }
  }

  /* Error messages */
  .customer.login .form__message {
    color: {{ section.settings.error_color }} !important;
    font-size: {{ section.settings.error_font_size }}px !important;
  }

  /* Guest login section */
  .customer.login hr {
    border-color: {{ section.settings.divider_color }} !important;
    margin: {{ section.settings.divider_margin_y }}px 0 !important;
  }

  .customer.login h2 {
    color: {{ section.settings.subheading_color }} !important;
    font-size: {{ section.settings.subheading_font_size }}px !important;
    text-align: {{ section.settings.text_alignment }} !important;
  }
{%- endstyle -%}

<div class="customer login section-{{ section.id }}-padding">
  <h1 id="recover" tabindex="-1">
    {{ 'customer.recover_password.title' | t }}
  </h1>
  <div>
    <p>
      {{ 'customer.recover_password.subtext' | t }}
    </p>

    {%- form 'recover_customer_password' -%}
      {% assign recover_success = form.posted_successfully? %}
      <div class="field">
        <input
          type="email"
          value=""
          name="email"
          id="RecoverEmail"
          autocorrect="off"
          autocapitalize="off"
          autocomplete="email"
          {% if form.errors %}
            aria-invalid="true"
            aria-describedby="RecoverEmail-email-error"
            autofocus
          {% endif %}
          placeholder="{{ 'customer.login_page.email' | t }}"
        >
        <label for="RecoverEmail">
          {{ 'customer.login_page.email' | t }}
        </label>
      </div>
      {%- if form.errors -%}
        <small id="RecoverEmail-email-error" class="form__message">
          <svg aria-hidden="true" focusable="false" viewBox="0 0 13 13">
            <circle cx="6.5" cy="6.50049" r="5.5" stroke="white" stroke-width="2"/>
            <circle cx="6.5" cy="6.5" r="5.5" fill="#EB001B" stroke="#EB001B" stroke-width="0.7"/>
            <path d="M5.87413 3.52832L5.97439 7.57216H7.02713L7.12739 3.52832H5.87413ZM6.50076 9.66091C6.88091 9.66091 7.18169 9.37267 7.18169 9.00504C7.18169 8.63742 6.88091 8.34917 6.50076 8.34917C6.12061 8.34917 5.81982 8.63742 5.81982 9.00504C5.81982 9.37267 6.12061 9.66091 6.50076 9.66091Z" fill="white"/>
            <path d="M5.87413 3.17832H5.51535L5.52424 3.537L5.6245 7.58083L5.63296 7.92216H5.97439H7.02713H7.36856L7.37702 7.58083L7.47728 3.537L7.48617 3.17832H7.12739H5.87413ZM6.50076 10.0109C7.06121 10.0109 7.5317 9.57872 7.5317 9.00504C7.5317 8.43137 7.06121 7.99918 6.50076 7.99918C5.94031 7.99918 5.46982 8.43137 5.46982 9.00504C5.46982 9.57872 5.94031 10.0109 6.50076 10.0109Z" fill="white" stroke="#EB001B" stroke-width="0.7">
          </svg>
          {{ form.errors.messages.form }}
        </small>
      {%- endif -%}

      <button>
        {{ 'customer.login_page.submit' | t }}
      </button>

      <a href="#login">
        {{ 'customer.login_page.cancel' | t }}
      </a>
    {%- endform -%}
  </div>

  <h1 id="login" tabindex="-1">
    {{ 'customer.login_page.title' | t }}
  </h1>
  <div>
    {%- if recover_success == true -%}
      <h3 class="form__message" tabindex="-1" autofocus>
        <svg aria-hidden="true" focusable="false" viewBox="0 0 13 13">
          <path d="M6.5 12.35C9.73087 12.35 12.35 9.73086 12.35 6.5C12.35 3.26913 9.73087 0.65 6.5 0.65C3.26913 0.65 0.65 3.26913 0.65 6.5C0.65 9.73086 3.26913 12.35 6.5 12.35Z" fill="#428445" stroke="white" stroke-width="0.7"/>
          <path d="M5.53271 8.66357L9.25213 4.68197" stroke="white"/>
          <path d="M4.10645 6.7688L6.13766 8.62553" stroke="white">
        </svg>
        {{ 'customer.recover_password.success' | t }}
      </h3>
    {%- endif -%}
    {%- form 'customer_login', novalidate: 'novalidate' -%}
      {%- if form.errors -%}
        <h2 class="form__message" tabindex="-1" autofocus>
          <span class="visually-hidden">{{ 'accessibility.error' | t }} </span>
          <svg aria-hidden="true" focusable="false" viewBox="0 0 13 13">
            <circle cx="6.5" cy="6.50049" r="5.5" stroke="white" stroke-width="2"/>
            <circle cx="6.5" cy="6.5" r="5.5" fill="#EB001B" stroke="#EB001B" stroke-width="0.7"/>
            <path d="M5.87413 3.52832L5.97439 7.57216H7.02713L7.12739 3.52832H5.87413ZM6.50076 9.66091C6.88091 9.66091 7.18169 9.37267 7.18169 9.00504C7.18169 8.63742 6.88091 8.34917 6.50076 8.34917C6.12061 8.34917 5.81982 8.63742 5.81982 9.00504C5.81982 9.37267 6.12061 9.66091 6.50076 9.66091Z" fill="white"/>
            <path d="M5.87413 3.17832H5.51535L5.52424 3.537L5.6245 7.58083L5.63296 7.92216H5.97439H7.02713H7.36856L7.37702 7.58083L7.47728 3.537L7.48617 3.17832H7.12739H5.87413ZM6.50076 10.0109C7.06121 10.0109 7.5317 9.57872 7.5317 9.00504C7.5317 8.43137 7.06121 7.99918 6.50076 7.99918C5.94031 7.99918 5.46982 8.43137 5.46982 9.00504C5.46982 9.57872 5.94031 10.0109 6.50076 10.0109Z" fill="white" stroke="#EB001B" stroke-width="0.7">
          </svg>
          {{ 'templates.contact.form.error_heading' | t }}
        </h2>
        {{ form.errors | default_errors }}
      {%- endif -%}

      <div class="field">
        <input
          type="email"
          name="customer[email]"
          id="CustomerEmail"
          autocomplete="email"
          autocorrect="off"
          autocapitalize="off"
          {% if form.errors contains 'form' %}
            aria-invalid="true"
          {% endif %}
          placeholder="{{ 'customer.login_page.email' | t }}"
        >
        <label for="CustomerEmail">
          {{ 'customer.login_page.email' | t }}
        </label>
      </div>

      {%- if form.password_needed -%}
        <div class="field">
          <input
            type="password"
            value=""
            name="customer[password]"
            id="CustomerPassword"
            autocomplete="current-password"
            {% if form.errors contains 'form' %}
              aria-invalid="true"
            {% endif %}
            placeholder="{{ 'customer.login_page.password' | t }}"
          >
          <label for="CustomerPassword">
            {{ 'customer.login_page.password' | t }}
          </label>
        </div>

        <a href="#recover">
          {{ 'customer.login_page.forgot_password' | t }}
        </a>
      {%- endif -%}

      <button>
        {{ 'customer.login_page.sign_in' | t }}
      </button>

      <a href="{{ routes.account_register_url }}">
        {{ 'customer.login_page.create_account' | t }}
      </a>
    {%- endform -%}
  </div>

  {%- if shop.checkout.guest_login -%}
    <div>
      <hr>
      <h2>{{ 'customer.login_page.guest_title' | t }}</h2>

      {%- form 'guest_login' -%}
        <button>
          {{ 'customer.login_page.guest_continue' | t }}
        </button>
      {%- endform -%}
    </div>
  {%- endif -%}
</div>

