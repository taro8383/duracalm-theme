{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  /* Cart Title Styling */
  .cart-items .title--primary {
    font-size: {{ section.settings.cart_title_size }}px;
    font-weight: {{ section.settings.cart_title_weight }};
    color: {{ section.settings.cart_title_color }};
    text-align: {{ section.settings.cart_title_alignment }};
    {% if section.settings.cart_title_uppercase %}text-transform: uppercase;{% endif %}
    letter-spacing: {{ section.settings.cart_title_letter_spacing }}em;
  }

  /* Continue Shopping Button - Magic Mind */
  {% if section.settings.enable_magic_mind_continue_button %}
  .title-wrapper-with-link .magic-mind-cart-btn {
    position: relative !important;
    display: inline-flex !important;
    align-items: center !important;
    justify-content: center !important;
    padding: {{ section.settings.continue_btn_padding_v }}px {{ section.settings.continue_btn_padding_h }}px !important;
    font-size: {{ section.settings.continue_btn_font_size }}px !important;
    font-weight: {{ section.settings.continue_btn_font_weight }} !important;
    letter-spacing: {{ section.settings.continue_btn_letter_spacing }}em !important;
    text-decoration: none !important;
    text-transform: {% if section.settings.continue_btn_uppercase %}uppercase{% else %}none{% endif %} !important;
    border: {{ section.settings.continue_btn_border_width }}px solid {{ section.settings.continue_btn_border_color }} !important;
    border-radius: {{ section.settings.continue_btn_border_radius }}px !important;
    color: {{ section.settings.continue_btn_text_color }} !important;
    background: {{ section.settings.continue_btn_bg_color }} !important;
    cursor: pointer !important;
    transition: all {{ section.settings.continue_btn_transition_speed }}s cubic-bezier(0.4, 0, 0.2, 1) !important;
    overflow: hidden !important;
    z-index: 1 !important;
    width: {% if section.settings.continue_btn_full_width %}100%{% else %}fit-content{% endif %} !important;
    box-shadow: {% if section.settings.continue_btn_shadow %}{{ section.settings.continue_btn_shadow_x }}px {{ section.settings.continue_btn_shadow_y }}px {{ section.settings.continue_btn_shadow_blur }}px {{ section.settings.continue_btn_shadow_color }}{% else %}none{% endif %} !important;
  }

  .title-wrapper-with-link .magic-mind-cart-btn::before {
    content: '' !important;
    position: absolute !important;
    top: 0 !important;
    left: 0 !important;
    width: 100% !important;
    height: 100% !important;
    background: linear-gradient(135deg, {{ section.settings.magic_mind_gradient_start }} 0%, {{ section.settings.magic_mind_gradient_mid }} 50%, {{ section.settings.magic_mind_gradient_end }} 100%) !important;
    background-size: 200% 200% !important;
    opacity: 0 !important;
    transition: opacity {{ section.settings.continue_btn_transition_speed }}s cubic-bezier(0.4, 0, 0.2, 1) !important;
    z-index: -1 !important;
  }

  .title-wrapper-with-link .magic-mind-cart-btn:hover::before {
    opacity: 1 !important;
    background-position: 100% 0 !important;
    animation: mm-gradient-shift-{{ section.id }} 0.6s ease forwards !important;
  }

  .title-wrapper-with-link .magic-mind-cart-btn:hover {
    color: {{ section.settings.continue_btn_hover_text_color }} !important;
    border-color: {{ section.settings.continue_btn_hover_border_color }} !important;
    transform: translateY(-{{ section.settings.continue_btn_hover_lift }}px) !important;
    box-shadow: {% if section.settings.continue_btn_hover_shadow %}{{ section.settings.continue_btn_hover_shadow_x }}px {{ section.settings.continue_btn_hover_shadow_y }}px {{ section.settings.continue_btn_hover_shadow_blur }}px {{ section.settings.continue_btn_hover_shadow_color }}{% else %}none{% endif %} !important;
    text-decoration: none !important;
    background: transparent !important;
  }

  @keyframes mm-gradient-shift-{{ section.id }} {
    0% { background-position: 0% 50%; }
    100% { background-position: 100% 50%; }
  }
  {% endif %}

  /* Standard Continue Shopping Link */
  .title-wrapper-with-link .underlined-link {
    color: {{ section.settings.continue_link_color }};
    font-size: {{ section.settings.continue_link_font_size }}px;
    font-weight: {{ section.settings.continue_link_font_weight }};
    text-decoration: {% if section.settings.continue_link_underline %}underline{% else %}none{% endif %};
  }

  .title-wrapper-with-link .underlined-link:hover {
    color: {{ section.settings.continue_link_hover_color }};
  }

  /* Empty Cart Styling */
  .cart__empty-text {
    font-size: {{ section.settings.empty_text_size }}px;
    color: {{ section.settings.empty_text_color }};
    font-weight: {{ section.settings.empty_text_weight }};
    text-align: {{ section.settings.empty_text_alignment }};
  }

  .cart__warnings .button {
    background-color: {{ section.settings.empty_btn_bg_color }};
    color: {{ section.settings.empty_btn_text_color }};
    border: {{ section.settings.empty_btn_border_width }}px solid {{ section.settings.empty_btn_border_color }};
    border-radius: {{ section.settings.empty_btn_border_radius }}px;
    padding: {{ section.settings.empty_btn_padding_v }}px {{ section.settings.empty_btn_padding_h }}px;
    font-size: {{ section.settings.empty_btn_font_size }}px;
    font-weight: {{ section.settings.empty_btn_font_weight }};
    text-transform: {% if section.settings.empty_btn_uppercase %}uppercase{% else %}none{% endif %};
  }

  .cart__warnings .button:hover {
    background-color: {{ section.settings.empty_btn_bg_hover_color }};
    color: {{ section.settings.empty_btn_text_hover_color }};
    border-color: {{ section.settings.empty_btn_border_hover_color }};
  }

  /* Cart Login Section */
  .cart__login-title {
    font-size: {{ section.settings.login_title_size }}px;
    color: {{ section.settings.login_title_color }};
    font-weight: {{ section.settings.login_title_weight }};
    {% if section.settings.login_title_uppercase %}text-transform: uppercase;{% endif %}
  }

  .cart__login-paragraph {
    font-size: {{ section.settings.login_text_size }}px;
    color: {{ section.settings.login_text_color }};
  }

  .cart__login-paragraph a {
    color: {{ section.settings.login_link_color }};
    text-decoration: {% if section.settings.login_link_underline %}underline{% else %}none{% endif %};
  }

  .cart__login-paragraph a:hover {
    color: {{ section.settings.login_link_hover_color }};
  }

  /* Table Headers */
  .cart-items th {
    font-size: {{ section.settings.table_header_font_size }}px;
    font-weight: {{ section.settings.table_header_font_weight }};
    color: {{ section.settings.table_header_color }};
    text-transform: {% if section.settings.table_header_uppercase %}uppercase{% else %}none{% endif %};
    letter-spacing: {{ section.settings.table_header_letter_spacing }}em;
    border-bottom: {{ section.settings.table_header_border_width }}px solid {{ section.settings.table_header_border_color }};
    padding-top: {{ section.settings.table_header_padding_v }}px;
    padding-bottom: {{ section.settings.table_header_padding_v }}px;
  }

  /* Cart Items Table */
  .cart-items {
    border-collapse: {% if section.settings.table_separate_borders %}separate{% else %}collapse{% endif %};
    {% if section.settings.table_border_width > 0 %}
    border: {{ section.settings.table_border_width }}px solid {{ section.settings.table_border_color }};
    {% endif %}
  }

  .cart-item {
    border-bottom: {{ section.settings.item_border_width }}px solid {{ section.settings.item_border_color }};
  }

  .cart-item:last-child {
    border-bottom: {% if section.settings.item_border_last_visible %}{{ section.settings.item_border_width }}px solid {{ section.settings.item_border_color }}{% else %}none{% endif %};
  }

  /* Product Image */
  .cart-item__image-container {
    width: {{ section.settings.image_width }}px;
    border-radius: {{ section.settings.image_border_radius }}px;
    overflow: hidden;
    {% if section.settings.image_border_width > 0 %}
    border: {{ section.settings.image_border_width }}px solid {{ section.settings.image_border_color }};
    {% endif %}
    {% if section.settings.image_shadow %}
    box-shadow: {{ section.settings.image_shadow_x }}px {{ section.settings.image_shadow_y }}px {{ section.settings.image_shadow_blur }}px {{ section.settings.image_shadow_color }};
    {% endif %}
  }

  .cart-item__image {
    width: 100%;
    height: {{ section.settings.image_height }}px;
    object-fit: {{ section.settings.image_object_fit }};
  }

  /* Product Vendor */
  .cart-item__details .caption-with-letter-spacing {
    font-size: {{ section.settings.vendor_font_size }}px;
    color: {{ section.settings.vendor_color }};
    font-weight: {{ section.settings.vendor_font_weight }};
    text-transform: {% if section.settings.vendor_uppercase %}uppercase{% else %}none{% endif %};
    letter-spacing: {{ section.settings.vendor_letter_spacing }}em;
    {% if section.settings.vendor_show == false %}display: none;{% endif %}
  }

  /* Product Title */
  .cart-item__name {
    font-size: {{ section.settings.title_font_size }}px;
    color: {{ section.settings.title_color }};
    font-weight: {{ section.settings.title_font_weight }};
    text-decoration: {% if section.settings.title_underline %}underline{% else %}none{% endif %};
    line-height: {{ section.settings.title_line_height }};
  }

  .cart-item__name:hover {
    color: {{ section.settings.title_hover_color }};
  }

  /* Product Price */
  .cart-item__details .product-option {
    font-size: {{ section.settings.price_font_size }}px;
    color: {{ section.settings.price_color }};
    font-weight: {{ section.settings.price_font_weight }};
  }

  .cart-item__old-price {
    font-size: {{ section.settings.price_old_font_size }}px;
    color: {{ section.settings.price_old_color }};
    text-decoration: line-through;
  }

  .cart-item__final-price {
    font-size: {{ section.settings.price_font_size }}px;
    color: {{ section.settings.price_sale_color }};
    font-weight: {{ section.settings.price_font_weight }};
  }

  /* Variant/Property Options */
  .cart-item__details dl .product-option {
    font-size: {{ section.settings.variant_font_size }}px;
    color: {{ section.settings.variant_color }};
    margin-bottom: {{ section.settings.variant_spacing }}px;
  }

  .cart-item__details dl .product-option dt {
    font-weight: {{ section.settings.variant_label_weight }};
    color: {{ section.settings.variant_label_color }};
  }

  /* Discounts */
  .cart-item__details .discounts__discount {
    font-size: {{ section.settings.discount_font_size }}px;
    color: {{ section.settings.discount_color }};
    background-color: {{ section.settings.discount_bg_color }};
    padding: {{ section.settings.discount_padding_v }}px {{ section.settings.discount_padding_h }}px;
    border-radius: {{ section.settings.discount_border_radius }}px;
    {% if section.settings.discount_show_icon == false %}
    .icon-discount { display: none; }
    {% endif %}
  }

  /* Quantity Selector */
  .cart-item__quantity .quantity {
    background-color: {{ section.settings.qty_bg_color }};
    border: {{ section.settings.qty_border_width }}px solid {{ section.settings.qty_border_color }};
    border-radius: {{ section.settings.qty_border_radius }}px;
    min-height: {{ section.settings.qty_height }}px;
  }

  .cart-item__quantity .quantity__button {
    color: {{ section.settings.qty_btn_color }};
    width: {{ section.settings.qty_btn_width }}px;
  }

  .cart-item__quantity .quantity__button:hover {
    color: {{ section.settings.qty_btn_hover_color }};
    background-color: {{ section.settings.qty_btn_hover_bg }};
  }

  .cart-item__quantity .quantity__input {
    color: {{ section.settings.qty_input_color }};
    font-size: {{ section.settings.qty_font_size }}px;
    font-weight: {{ section.settings.qty_font_weight }};
  }

  /* Remove Button */
  .cart-item__quantity .button--tertiary {
    color: {{ section.settings.remove_btn_color }};
    background-color: {{ section.settings.remove_btn_bg_color }};
    border: {{ section.settings.remove_btn_border_width }}px solid {{ section.settings.remove_btn_border_color }};
    border-radius: {{ section.settings.remove_btn_border_radius }}px;
    width: {{ section.settings.remove_btn_size }}px;
    height: {{ section.settings.remove_btn_size }}px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .cart-item__quantity .button--tertiary:hover {
    color: {{ section.settings.remove_btn_hover_color }};
    background-color: {{ section.settings.remove_btn_hover_bg_color }};
    border-color: {{ section.settings.remove_btn_hover_border_color }};
  }

  /* Line Item Totals */
  .cart-item__price-wrapper .price {
    font-size: {{ section.settings.line_total_font_size }}px;
    color: {{ section.settings.line_total_color }};
    font-weight: {{ section.settings.line_total_font_weight }};
  }

  .cart-item__price-wrapper .cart-item__old-price.price {
    font-size: {{ section.settings.line_total_old_font_size }}px;
    color: {{ section.settings.line_total_old_color }};
  }

  /* Unit Price */
  .cart-item__totals .unit-price {
    font-size: {{ section.settings.unit_price_font_size }}px;
    color: {{ section.settings.unit_price_color }};
    margin-top: {{ section.settings.unit_price_margin_top }}px;
  }

  /* Error Messages */
  .cart-item__error {
    color: {{ section.settings.error_color }};
    font-size: {{ section.settings.error_font_size }}px;
  }

  /* Remove arrows from any button */
  .title-wrapper-with-link .magic-mind-cart-btn::after,
  .title-wrapper-with-link .magic-mind-cart-btn .icon-arrow,
  .title-wrapper-with-link .magic-mind-cart-btn svg {
    display: none !important;
    content: none !important;
    background: none !important;
    background-image: none !important;
  }
{%- endstyle -%}

<cart-items class="page-width{% if cart == empty %} is-empty{% else %} section-{{ section.id }}-padding{% endif %}">
  <div class="title-wrapper-with-link title-wrapper-with-link--spacing">
    <h1 class="title title--primary">{{ 'sections.cart.title' | t }}</h1>
    {%- if section.settings.show_continue_shopping -%}
      {%- if section.settings.enable_magic_mind_continue_button -%}
        <a href="{% if settings.continue_shopping_url == blank %}{{ routes.all_products_collection_url }}{% else %}{{ settings.continue_shopping_url }}{% endif %}" class="magic-mind-cart-btn">
          {{ 'general.continue_shopping' | t }}
        </a>
      {%- else -%}
        <a href="{% if settings.continue_shopping_url == blank %}{{ routes.all_products_collection_url }}{% else %}{{ settings.continue_shopping_url }}{% endif %}" class="underlined-link">
          {{ 'general.continue_shopping' | t }}
        </a>
      {%- endif -%}
    {%- endif -%}
  </div>

  <div class="cart__warnings">
    {%- if section.settings.show_empty_icon -%}
      <div class="cart__empty-icon" style="text-align: center; margin-bottom: 20px;">
        <svg xmlns="http://www.w3.org/2000/svg" width="{{ section.settings.empty_icon_size }}" height="{{ section.settings.empty_icon_size }}" viewBox="0 0 24 24" fill="none" stroke="{{ section.settings.empty_icon_color }}" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
          <circle cx="9" cy="21" r="1"></circle>
          <circle cx="20" cy="21" r="1"></circle>
          <path d="M1 1h4l2.68 13.39a2 2 0 0 0 2 1.61h9.72a2 2 0 0 0 2-1.61L23 6H6"></path>
        </svg>
      </div>
    {%- endif -%}
    <h1 class="cart__empty-text">{{ section.settings.empty_text | default: 'sections.cart.empty' | t }}</h1>
    {% if section.settings.show_empty_button and settings.display_continue_shopping %}
      <a href="{% if settings.continue_shopping_url == blank %}{{ routes.all_products_collection_url }}{% else %}{{ settings.continue_shopping_url }}{% endif %}" class="button">
        {{ section.settings.empty_button_text | default: 'general.continue_shopping' | t }}
      </a>
    {% endif %}

    {%- if shop.customer_accounts_enabled and customer == null and section.settings.show_login_section -%}
      <h2 class="cart__login-title">{{ 'sections.cart.login.title' | t }}</h2>
      <p class="cart__login-paragraph">
        {{ 'sections.cart.login.paragraph_html' | t: link: routes.account_login_url }}
      </p>
    {%- endif -%}
  </div>

  <form action="{{ routes.cart_url }}" class="cart__contents critical-hidden" method="post" id="cart">
    <div class="cart__items" id="main-cart-items" data-id="{{ section.id }}">
      <div class="js-contents">
        {%- if cart != empty -%}
          <table class="cart-items">
            <caption class="visually-hidden">
              {{ 'sections.cart.title' | t }}
            </caption>
            <thead>
              <tr>
                <th class="caption-with-letter-spacing" colspan="2" scope="col">
                  {{ 'sections.cart.headings.product' | t }}
                </th>
                <th class="medium-hide large-up-hide right caption-with-letter-spacing" colspan="1" scope="col">
                  {{ 'sections.cart.headings.total' | t }}
                </th>
                <th class="cart-items__heading--wide small-hide caption-with-letter-spacing" colspan="1" scope="col">
                  {{ 'sections.cart.headings.quantity' | t }}
                </th>
                <th class="small-hide right caption-with-letter-spacing" colspan="1" scope="col">
                  {{ 'sections.cart.headings.total' | t }}
                </th>
              </tr>
            </thead>

            <tbody>
              {%- for item in cart.items -%}
                <tr
                  class="cart-item cart-item--product-{{ item.product.handle }}"
                  id="CartItem-{{ item.index | plus: 1 }}"
                  data-quantity="{{ item.quantity }}"
                >
                  <td class="cart-item__media">
                    {% if item.image %}
                      {% comment %} Leave empty space due to a:empty CSS display: none rule {% endcomment %}
                      <a href="{{ item.url }}" class="cart-item__link" aria-hidden="true" tabindex="-1"> </a>
                      <div class="cart-item__image-container gradient global-media-settings">
                        <img
                          src="{{ item.image | image_url: width: 300 }}"
                          class="cart-item__image"
                          alt="{{ item.image.alt | escape }}"
                          loading="lazy"
                          width="150"
                          height="{{ 150 | divided_by: item.image.aspect_ratio | ceil }}"
                        >
                      </div>
                    {% endif %}
                  </td>

                  <td class="cart-item__details">
                    {%- if settings.show_vendor -%}
                      <p class="caption-with-letter-spacing">{{ item.product.vendor }}</p>
                    {%- endif -%}

                    <a href="{{ item.url }}" class="cart-item__name h4 break">{{ item.product.title | escape }}</a>

                    {%- if item.original_price != item.final_price -%}
                      <div class="cart-item__discounted-prices">
                        <span class="visually-hidden">
                          {{ 'products.product.price.regular_price' | t }}
                        </span>
                        <s class="cart-item__old-price product-option">
                          {{- item.original_price | money -}}
                        </s>
                        <span class="visually-hidden">
                          {{ 'products.product.price.sale_price' | t }}
                        </span>
                        <strong class="cart-item__final-price product-option">
                          {{ item.final_price | money }}
                        </strong>
                      </div>
                    {%- else -%}
                      <div class="product-option">
                        {{ item.original_price | money }}
                      </div>
                    {%- endif -%}

                    {%- if item.product.has_only_default_variant == false
                      or item.properties.size != 0
                      or item.selling_plan_allocation != null
                    -%}
                      <dl>
                        {%- if item.product.has_only_default_variant == false -%}
                          {%- for option in item.options_with_values -%}
                            <div class="product-option">
                              <dt>{{ option.name }}:</dt>
                              <dd>{{ option.value }}</dd>
                            </div>
                          {%- endfor -%}
                        {%- endif -%}

                        {%- for property in item.properties -%}
                          {%- assign property_first_char = property.first | slice: 0 -%}
                          {%- if property.last != blank and property_first_char != '_' -%}
                            <div class="product-option">
                              <dt>{{ property.first }}:</dt>
                              <dd>
                                {%- if property.last contains '/uploads/' -%}
                                  <a href="{{ property.last }}" class="link" target="_blank">
                                    {{ property.last | split: '/' | last }}
                                  </a>
                                {%- else -%}
                                  {{ property.last }}
                                {%- endif -%}
                              </dd>
                            </div>
                          {%- endif -%}
                        {%- endfor -%}
                      </dl>

                      <p class="product-option">{{ item.selling_plan_allocation.selling_plan.name }}</p>
                    {%- endif -%}

                    <ul class="discounts list-unstyled" role="list" aria-label="{{ 'customer.order.discount' | t }}">
                      {%- for discount in item.discounts -%}
                        <li class="discounts__discount">
                          {%- render 'icon-discount' -%}
                          {{ discount.title }}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </td>

                  <td class="cart-item__totals right medium-hide large-up-hide">
                    <div class="loading-overlay hidden">
                      <div class="loading-overlay__spinner">
                        <svg
                          aria-hidden="true"
                          focusable="false"
                          class="spinner"
                          viewBox="0 0 66 66"
                          xmlns="http://www.w3.org/2000/svg"
                        >
                          <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                        </svg>
                      </div>
                    </div>
                    <div class="cart-item__price-wrapper">
                      {%- if item.original_line_price != item.final_line_price -%}
                        <dl class="cart-item__discounted-prices">
                          <dt class="visually-hidden">
                            {{ 'products.product.price.regular_price' | t }}
                          </dt>
                          <dd>
                            <s class="cart-item__old-price price price--end">
                              {{ item.original_line_price | money }}
                            </s>
                          </dd>
                          <dt class="visually-hidden">
                            {{ 'products.product.price.sale_price' | t }}
                          </dt>
                          <dd class="price price--end">
                            {{ item.final_line_price | money }}
                          </dd>
                        </dl>
                      {%- else -%}
                        <span class="price price--end">
                          {{ item.original_line_price | money }}
                        </span>
                      {%- endif -%}

                      {%- if item.variant.available and item.unit_price_measurement -%}
                        <div class="unit-price caption">
                          <span class="visually-hidden">{{ 'products.product.price.unit_price' | t }}</span>
                          {{ item.variant.unit_price | money }}
                          <span aria-hidden="true">/</span>
                          <span class="visually-hidden"
                            >&nbsp;{{ 'accessibility.unit_price_separator' | t }}&nbsp;</span
                          >
                          {%- if item.variant.unit_price_measurement.reference_value != 1 -%}
                            {{- item.variant.unit_price_measurement.reference_value -}}
                          {%- endif -%}
                          {{ item.variant.unit_price_measurement.reference_unit }}
                        </div>
                      {%- endif -%}
                    </div>
                  </td>

                  <td class="cart-item__quantity">
                    <div class="cart-item__quantity-wrapper">
                      <label class="visually-hidden" for="Quantity-{{ item.index | plus: 1 }}">
                        {{ 'products.product.quantity.label' | t }}
                      </label>
                      <quantity-input class="quantity main-quantity cart-quantity no-background color-{{ settings.quantity_color_scheme }} accent-color-{{ settings.quantity_overlay_color }} accent-2-color-{{ settings.quantity_text_color }}">
                        <button class="quantity__button no-js-hidden" name="minus" type="button">
                          <span class="visually-hidden">
                            {{- 'products.product.quantity.decrease' | t: product: item.product.title | escape -}}
                          </span>
                          {% render 'icon-minus' %}
                        </button>
                        <input
                          class="quantity__input"
                          data-quantity-variant-id="{{ item.variant.id }}"
                          type="number"
                          name="updates[]"
                          value="{{ item.quantity }}"
                          {% # theme-check-disable %}
                          data-cart-quantity="{{ cart | item_count_for_variant: item.variant.id }}"
                          min="{{ item.variant.quantity_rule.min }}"
                          {% if item.variant.quantity_rule.max != null %}
                            max="{{ item.variant.quantity_rule.max }}"
                          {% endif %}
                          step="{{ item.variant.quantity_rule.increment }}"
                          {% # theme-check-enable %}
                          aria-label="{{ 'products.product.quantity.input_label' | t: product: item.product.title | escape }}"
                          id="Quantity-{{ item.index | plus: 1 }}"
                          data-index="{{ item.index | plus: 1 }}"
                        >
                        <button class="quantity__button no-js-hidden" name="plus" type="button">
                          <span class="visually-hidden">
                            {{- 'products.product.quantity.increase' | t: product: item.product.title | escape -}}
                          </span>
                          {% render 'icon-plus' %}
                        </button>
                      </quantity-input>

                      <cart-remove-button
                        id="Remove-{{ item.index | plus: 1 }}"
                        data-index="{{ item.index | plus: 1 }}"
                      >
                        <a
                          href="{{ item.url_to_remove }}"
                          class="button button--tertiary"
                          aria-label="{{ 'sections.cart.remove_title' | t: title: item.title }}"
                        >
                          {% render 'icon-remove' %}
                        </a>
                      </cart-remove-button>
                    </div>
                    <div class="cart-item__error" id="Line-item-error-{{ item.index | plus: 1 }}" role="alert">
                      <small class="cart-item__error-text"></small>
                      <svg
                        aria-hidden="true"
                        focusable="false"
                        class="icon icon-error"
                        viewBox="0 0 13 13"
                      >
                        <circle cx="6.5" cy="6.50049" r="5.5" stroke="white" stroke-width="2"/>
                        <circle cx="6.5" cy="6.5" r="5.5" fill="#EB001B" stroke="#EB001B" stroke-width="0.7"/>
                        <path d="M5.87413 3.52832L5.97439 7.57216H7.02713L7.12739 3.52832H5.87413ZM6.50076 9.66091C6.88091 9.66091 7.18169 9.37267 7.18169 9.00504C7.18169 8.63742 6.88091 8.34917 6.50076 8.34917C6.12061 8.34917 5.81982 8.63742 5.81982 9.00504C5.81982 9.37267 6.12061 9.66091 6.50076 9.66091Z" fill="white"/>
                        <path d="M5.87413 3.17832H5.51535L5.52424 3.537L5.6245 7.58083L5.63296 7.92216H5.97439H7.02713H7.36856L7.37702 7.58083L7.47728 3.537L7.48617 3.17832H7.12739H5.87413ZM6.50076 10.0109C7.06121 10.0109 7.5317 9.57872 7.5317 9.00504C7.5317 8.43137 7.06121 7.99918 6.50076 7.99918C5.94031 7.99918 5.46982 8.43137 5.46982 9.00504C5.46982 9.57872 5.94031 10.0109 6.50076 10.0109Z" fill="white" stroke="#EB001B" stroke-width="0.7">
                      </svg>
                    </div>
                  </td>

                  <td class="cart-item__totals right small-hide">
                    <div class="loading-overlay hidden">
                      <div class="loading-overlay__spinner">
                        <svg
                          aria-hidden="true"
                          focusable="false"
                          class="spinner"
                          viewBox="0 0 66 66"
                          xmlns="http://www.w3.org/2000/svg"
                        >
                          <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                        </svg>
                      </div>
                    </div>

                    <div class="cart-item__price-wrapper">
                      {%- if item.original_line_price != item.final_line_price -%}
                        <dl class="cart-item__discounted-prices">
                          <dt class="visually-hidden">
                            {{ 'products.product.price.regular_price' | t }}
                          </dt>
                          <dd>
                            <s class="cart-item__old-price price price--end">
                              {{ item.original_line_price | money }}
                            </s>
                          </dd>
                          <dt class="visually-hidden">
                            {{ 'products.product.price.sale_price' | t }}
                          </dt>
                          <dd class="price price--end">
                            {{ item.final_line_price | money }}
                          </dd>
                        </dl>
                      {%- else -%}
                        <span class="price price--end">
                          {{ item.original_line_price | money }}
                        </span>
                      {%- endif -%}

                      {%- if item.variant.available and item.unit_price_measurement -%}
                        <div class="unit-price caption">
                          <span class="visually-hidden">{{ 'products.product.price.unit_price' | t }}</span>
                          {{ item.variant.unit_price | money }}
                          <span aria-hidden="true">/</span>
                          <span class="visually-hidden"
                            >&nbsp;{{ 'accessibility.unit_price_separator' | t }}&nbsp;</span
                          >
                          {%- if item.variant.unit_price_measurement.reference_value != 1 -%}
                            {{- item.variant.unit_price_measurement.reference_value -}}
                          {%- endif -%}
                          {{ item.variant.unit_price_measurement.reference_unit }}
                        </div>
                      {%- endif -%}
                    </div>
                  </td>
                </tr>
              {%- endfor -%}
            </tbody>
          </table>
        {%- endif -%}
      </div>
    </div>

    <p class="visually-hidden" id="cart-live-region-text" aria-live="polite" role="status"></p>
    <p class="visually-hidden" id="shopping-cart-line-item-status" aria-live="polite" aria-hidden="true" role="status">
      {{ 'accessibility.loading' | t }}
    </p>
  </form>
</cart-items>

